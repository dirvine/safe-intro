<h2 class="col-sm-offset-3 main">Dashboard</h2>


<h2 class="col-sm-offset-3 main" >Articles</h2>
<div class="col-sm-9 col-sm-offset-3 col-md-10 col-md-offset-2 main">
  <div class="table-responsive">
    <table class="table table-striped">
      <thead>
        <tr>
          <th>#</th>
          <th>Header</th>
          <th>Header</th>
          <th>Header</th>
          <th>Header</th>
        </tr>
      </thead>
      <tbody>
        {{#each articles as |article|}}
        <tr>
          <td>{{ article.[0] }}</td>
          <td>{{ article.[1] }}</td>
          <td>{{ article.[2] }}</td>
          <td>{{ article.[3] }}</td>
          <td>{{ article.[4] }}</td>
        </tr>
        {{/each}}
      </tbody>
    </table>
  </div>
</div>
